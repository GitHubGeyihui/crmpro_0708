    <resultMap id="newProject" type="com.ujiuye.pro.bean.Project" extends="BaseResultMap">
        <association property="customer" column="comname" javaType="com.ujiuye.cus.bean.Customer"
                     select="com.ujiuye.cus.mapper.CustomerMapper.selectByPrimaryKey"/>
        <association property="employee" column="emp_fk" javaType="com.ujiuye.emp.bean.Employee"
                     select="com.ujiuye.emp.mapper.EmployeeMapper.selectByPrimaryKey"/>
        <association property="analysis" column="pid" javaType="com.ujiuye.pro.bean.Analysis"
                     select="com.ujiuye.pro.mapper.AnalysisMapper.selectByPrimaryKey"/>
    </resultMap>

    <select id="getProAndOtherInfo" parameterType="com.ujiuye.util.MarkInfo" resultMap="newProject">
        select * from project
        <if test="mark == 0">
            where pid not in (select id from analysis)
        </if>
        <if test="mark == 1">
            where pid in (select id from analysis)
        </if>
        <if test="mark==2">
            where pid in (select distinct analysis_fk from module )
        </if>
        <!--     有用户信息，而且功能是没有被分配的任务-->
        <if test="mark==3">
            where pid in (select  distinct analysis_fk from module where id in (select distinct modele_fk from function where id not in (select fun_fk from task)));
        </if>
    </select>
</mapper>